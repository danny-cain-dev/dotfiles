# Hive - Proxmox server
Host hive hive.cain.mobi
    Hostname hive.cain.mobi
    User danny
    ForwardAgent yes

Host hive-web web.hive.cain.mobi web.cain.mobi
    Hostname 172.16.16.2
    ProxyJump hive.cain.mobi
    User danny
    ForwardAgent yes

Host hive-llama llama.hive.cain.mobi llama.cain.mobi
    Hostname 172.16.16.3
    ProxyJump hive.cain.mobi
    User danny
    ForwardAgent yes

Host hive-git git.hive.cain.mobi git.cain.mobi
    Hostname git.hive.cain.mobi
    ProxyJump hive.cain.mobi
    User danny
    ForwardAgent yes

